<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="dev.tsnanh.education.englishvocabularywithpictures">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="dev.tsnanh.education.englishvocabularywithpictures.controller.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:targetApi="n">
        <activity android:name="dev.tsnanh.education.englishvocabularywithpictures.view.SplashActivity" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="dev.tsnanh.education.englishvocabularywithpictures.view.TestVocabularyActivity" android:exported="false" />
        <activity android:name="dev.tsnanh.education.englishvocabularywithpictures.view.MatchingGameActivity" android:exported="false" />
        <activity android:name="dev.tsnanh.education.englishvocabularywithpictures.view.LearnActivity" android:exported="false" />
        <activity android:name="dev.tsnanh.education.englishvocabularywithpictures.view.VocabularyAll" android:exported="false" />
        <activity android:name="dev.tsnanh.education.englishvocabularywithpictures.view.FavoriteVocabularyActivity" android:exported="false" />
        <activity android:name="dev.tsnanh.education.englishvocabularywithpictures.view.VocabularySearchActivity" android:exported="false" />
        <activity android:name="dev.tsnanh.education.englishvocabularywithpictures.view.VocabularySettingsActivity" android:exported="false" />
        <activity android:name="dev.tsnanh.education.englishvocabularywithpictures.view.VocabularyActivity" android:exported="false" />
        <activity android:name="dev.tsnanh.education.englishvocabularywithpictures.view.VocabularyCategoryActivity" android:exported="false" />
        <activity android:name="dev.tsnanh.education.englishvocabularywithpictures.view.MainActivity" android:exported="false">
        </activity>
    </application>

</manifest>